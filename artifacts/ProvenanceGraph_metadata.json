			"methods": {
				"addContributorEdge(uint256,address,uint16)": {
					"details": "Can only be called by the owner of the `_assetId`.",
					"params": {
						"_assetId": "The token ID of the asset.",
						"_contributor": "The address of the contributor.",
						"_weightBps": "The share of the contributor in basis points (1-10000)."
					}
				},
				"addParentEdge(uint256,uint256,uint16)": {
					"details": "Can only be called by the owner of the `_childAssetId`.",
					"params": {
						"_childAssetId": "The token ID of the child asset.",
						"_parentAssetId": "The token ID of the parent asset.",
						"_weightBps": "The share of the parent asset in basis points (1-10000)."
					}
				},
				"finalize(uint256)": {
					"details": "Can only be called by the owner of the `_assetId`. The total weight must be less than or equal to 10000 bps.",
					"params": {
						"_assetId": "The token ID of the asset to finalize."
					}
				}
			},
			"title": "ProvenanceGraph",
			"version": 1
		},
		"userdoc": {
			"kind": "user",
			"methods": {
				"addContributorEdge(uint256,address,uint16)": {
					"notice": "Adds a contributor and their contribution weight to an asset's graph."
				},
				"addParentEdge(uint256,uint256,uint16)": {
					"notice": "Adds a parent asset and its contribution weight to a child asset's graph."
				},
				"finalize(uint256)": {
					"notice": "Finalizes the provenance graph for an asset, locking it permanently."
				},
				"getContributorEdges(uint256)": {
					"notice": "Retrieves all contributor edges for a given asset."
				},
				"getParentEdges(uint256)": {
					"notice": "Retrieves all parent asset edges for a given asset."
				},
				"getTotalBpsAllocated(uint256)": {
					"notice": "Returns the total basis points allocated for a given asset."
				},
				"isFinalized(uint256)": {
					"notice": "Returns true if the graph for a given asset has been finalized."
				}
			},
			"notice": "Manages the contribution graph for assets, linking them to contributor wallets and parent assets. The owner of an asset is responsible for defining and finalizing its provenance.",
			"version": 1
		}
